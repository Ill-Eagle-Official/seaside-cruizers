{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "functions": {
    "api/webhook.js": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "dashsheet.html"
    }
  }
}